{
    "name": "Ekşi Return To Innocence",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Ekşi Sözlüğü Masumiyete Döndürür",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "Ekşi Return To Innocence"
     },
     "icons": {
        "128": "img/icon.png"
     },
    "content_scripts": [
        {
            "all_frames": true,
            "js": [
                "content.js"
            ],
            "matches": [
                "https://eksisozluk.com/*",
                "https://www.eksisozluk.com/*"

            ],
            "run_at": "document_start"
        }
    ]
}